  },
  "homepage": "https://github.com/paradox41/color-scheme-parser#readme",
  "devDependencies": {
    "babel-cli": "^6.4.0",
    "babel-core": "^6.4.0",
    "babel-eslint": "^4.1.6",
    "babel-preset-es2015": "^6.3.13",
    "chai": "^3.5.0",
    "chai-spies": "^0.7.1",
    "coveralls": "^2.11.6",
    "doctoc": "^0.15.0",
    "eslint": "^1.10.3",
    "eslint-plugin-import": "^0.12.1",
    "isparta": "^4.0.0",
    "istanbul": "^0.4.2",
    "lodash": "^4.0.0",
    "mocha": "^2.4.5",
    "rimraf": "^2.5.0",
    "watch": "^0.17.1"
  },
  "dependencies": {
    "commander": "^2.9.0",
    "mustache": "^2.2.1",
    "node-uuid": "^1.4.7",
    "yamljs": "^0.2.4"
  }
