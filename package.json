{
  "name": "color-scheme-parser",
  "version": "4.0.0",
  "description": "Convert Atom color schemes to Sublime color scheme",
  "main": "src/index.js",
  "bin": {
    "color-schemer": "./cli.js"
  },
  "files": [
    "dist/",
    "templates/"
  ],
  "scripts": {
    "start": "node -r babel-register cli.js ../pulley-syntax/index.less",
    "clean": "rimraf dist",
    "lint": "eslint src test",
    "prebuild": "npm run clean",
    "test": "mocha --compilers js:babel-core/register",
    "build": "babel src -d dist",
    "postbuild": "npm run test",
    "coverage": "babel-node ./node_modules/.bin/isparta cover _mocha",
    "coveralls": "npm run coverage && coveralls < coverage/lcov.info",
    "postcoveralls": "rimraf ./coverage",
    "docs": "documentation build ./src/index.js --format html --output ./docs",
    "prerelease": "npm run build",
    "release": "standard-version && git push --follow-tags origin master",
    "postrelease": "npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/paradox41/color-scheme-parser.git"
  },
  "keywords": [
    "node"
  ],
  "author": "Will Soto <will.soto9+github@gmail.com>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/paradox41/color-scheme-parser/issues"
  },
  "homepage": "https://github.com/paradox41/color-scheme-parser#readme",
  "dependencies": {
    "css": "^2.2.1",
    "first-mate": "^5.1.1",
    "less": "^2.7.1",
    "lodash": "^4.13.1",
    "mustache": "^2.2.1",
    "node-uuid": "^1.4.7",
    "plist": "^1.2.0",
    "tinycolor2": "^1.4.1",
    "yargs": "^4.7.1"
  },
  "devDependencies": {
    "babel-core": "^6.10.4",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-register": "^6.9.0",
    "chai": "^3.5.0",
    "chai-spies": "^0.7.1",
    "coveralls": "^2.11.6",
    "documentation": "^4.0.0-beta2",
    "eslint": "^3.0.1",
    "eslint-plugin-import": "^1.10.2",
    "isparta": "^4.0.0",
    "istanbul": "^0.4.2",
    "js-yaml": "^3.6.1",
    "mocha": "^2.4.5",
    "rimraf": "^2.5.0",
    "standard-version": "^2.3.0"
  }
}
